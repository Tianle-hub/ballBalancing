<?xml version="1.0"?>

<launch>

  <!-- ur10 settings -->
  <arg name="CONFIG_FILE_PATH" default="$(find ur10_controller)/launch/configs/configUR10.ini"/>

  <!-- the controller -->
  <node name="ur10_effort_controller" pkg="ur10_controller" type="ur10_effort_controller_node" args="$(arg CONFIG_FILE_PATH)" output="screen">

    <!-- internal parameter --> 
    <rosparam file="$(find tum_ics_ur_robot_controllers)/launch/configs/pidInt.yaml" command="load" />

    <!-- controller parameter --> 
    <rosparam file="$(find ur10_controller)/launch/configs/ur10EffortCtrl.yaml" command="load" />

    <rosparam file="$(find ball_controller)/configs/ball_controller.yaml" command="load" />
  </node>

  <!-- <node name="ball_blob_detector" pkg="ball_blob_detector" type="ball_blob_detector_node"/> -->

  

</launch>
